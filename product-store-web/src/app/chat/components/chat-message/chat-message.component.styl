.msgContainer
  padding: 10px 10px 35px
  border-top: 1px solid #e4e4e4
  color: #333

  .userIcon
    font-size: 35px

  .user
    margin: 0px 10px
    position: relative
    bottom: 5px

  .message
    margin-top: 10px

  pre
    display: block
    padding: 0
    color: #333
    font-size: 14px
    background-color: inherit
    border: none
    font-family: inherit

  .timeContainer
    font-size: 10px
    float: right
.header
  position: relative
  span
    position: absolute
    top: 8px
    padding-left: 10px